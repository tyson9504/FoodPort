@model IEnumerable<FinalProject_FoodPort.Models.AdminModel>

@{
    ViewBag.Title = "ViewAdmin";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<div align="center" >
    <h2>All Admins</h2>

    <table class="table table-hover table-bordered">
        <tr class="success">
            <th style="text-align:center">
                @Html.DisplayNameFor(model => model.AdminID)
            </th>
            <th style="text-align:center">
                @Html.DisplayNameFor(model => model.AdminName)
            </th>
            <th style="text-align:center">
                @Html.DisplayNameFor(model => model.AdminPhoneNumber)
            </th>
            <th style="text-align:center">
                @Html.DisplayNameFor(model => model.AdminEmailID)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td style="text-align:center">
                    @Html.DisplayFor(modelItem => item.AdminID)
                </td>
                <td style="text-align:center">
                    @Html.DisplayFor(modelItem => item.AdminName)
                </td>
                <td style="text-align:center">
                    @Html.DisplayFor(modelItem => item.AdminPhoneNumber)
                </td>
                <td style="text-align:center">
                    @Html.DisplayFor(modelItem => item.AdminEmailID)
                </td>
            </tr>
        }

    </table>
</div>